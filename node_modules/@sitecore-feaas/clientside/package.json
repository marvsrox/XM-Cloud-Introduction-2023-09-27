{
  "name": "@sitecore-feaas/clientside",
  "version": "0.1.51",
  "scripts": {
    "start": "webpack serve --open",
    "build": "rm -rf dist;npm run types; npm run build:esbuild; npm run build:sourcemaps",
    "build:esbuild": "node scripts/build.js",
    "build:sourcemaps": "npx merge-source-maps dist/browser/*.js --inline-sources",
    "test": "npx vitest ",
    "test:watch": "npx vitest ",
    "prepublishOnly": "npm run build",
    "check": "npx tsc --noEmit",
    "types": "tsc --build",
    "postpublish": "node ../cdn/upload-package.js clientside $npm_package_version"
  },
  "license": "ISC",
  "peerDependencies": {
    "react-dom": ">=16.8.0"
  },
  "main": "./dist/node/index.cjs",
  "browser": "./dist/browser/index.cjs",
  "module": "./dist/node/index.esm.js",
  "types": "./types/ui/index.d.js",
  "type": "module",
  "exports": {
    ".": {
      "localhost": "./build/ui/index.js",
      "types": "./types/ui/index.d.ts",
      "browser": "./dist/browser/index.esm.js",
      "import": "./dist/node/index.esm.js",
      "require": "./dist/node/index.cjs",
      "default": "./dist/node/index.esm.js"
    },
    "./headless": {
      "localhost": "./build/headless.js",
      "types": "./types/headless.d.ts",
      "browser": "./dist/browser/headless.esm.js",
      "import": "./dist/node/headless.esm.js",
      "require": "./dist/node/headless.cjs",
      "default": "./dist/node/headless.esm.js"
    },
    "./react": {
      "localhost": "./build/ui/react.js",
      "types": "./types/ui/react.d.ts",
      "browser": "./dist/browser/react.esm.js",
      "import": "./dist/node/react.esm.js",
      "require": "./dist/node/react.cjs",
      "default": "./dist/node/react.esm.js"
    },
    "./dist/*": {
      "types": "./types/ui/index.d.ts",
      "default": "./dist/*"
    },
    "./react.cjs": {
      "types": "./react.d.ts",
      "default": "./react.cjs"
    }
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "FEAAS Clientside"
  },
  "devDependencies": {
    "@types/jsonpath-plus": "^5.0.2",
    "@vitest/browser": "^0.29.2",
    "esbuild": "^0.16.17",
    "esbuild-plugin-replace-regex": "^0.0.2",
    "htmlparser2": "^8.0.1",
    "jsonpath-plus": "^7.2.0",
    "merge-source-maps": "^0.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
